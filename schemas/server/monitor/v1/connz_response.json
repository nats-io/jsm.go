{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://nats.io/schemas/server/monitor/v1/connz_response.json",
  "description": "A response from the NATS $SYS monitoring endpoint for connection information",
  "title": "io.nats.server.monitor.v1.connz_response",
  "type": "object",
  "required": [
    "server_id",
    "now",
    "num_connections",
    "total",
    "offset",
    "limit",
    "connections"
  ],
  "properties": {
    "server_id": {
      "type": "string",
      "description": "The unique server ID"
    },
    "now": {
      "type": "string",
      "format": "date-time",
      "description": "The current time on the server"
    },
    "num_connections": {
      "type": "integer",
      "minimum": 0,
      "description": "The number of active connections"
    },
    "total": {
      "type": "integer",
      "minimum": 0,
      "description": "The total number of connections available"
    },
    "offset": {
      "type": "integer",
      "minimum": 0,
      "description": "The pagination offset used"
    },
    "limit": {
      "type": "integer",
      "minimum": 0,
      "description": "The pagination limit used"
    },
    "connections": {
      "type": "array",
      "description": "List of connection details",
      "items": {
        "type": "object",
        "properties": {
          "cid": {
            "type": "integer",
            "description": "Connection ID"
          },
          "kind": {
            "type": "string",
            "description": "Connection type (Client, Router, Gateway, Leafnode)"
          },
          "type": {
            "type": "string",
            "description": "Connection protocol type"
          },
          "ip": {
            "type": "string",
            "description": "Remote IP address"
          },
          "port": {
            "type": "integer",
            "description": "Remote port"
          },
          "start": {
            "type": "string",
            "format": "date-time",
            "description": "Connection start time"
          },
          "last_activity": {
            "type": "string",
            "format": "date-time",
            "description": "Last activity time"
          },
          "stop": {
            "type": "string",
            "format": "date-time",
            "description": "Connection stop time (if closed)"
          },
          "reason": {
            "type": "string",
            "description": "Disconnection reason (if closed)"
          },
          "rtt": {
            "type": "string",
            "description": "Round trip time"
          },
          "uptime": {
            "type": "string",
            "description": "Connection uptime"
          },
          "idle": {
            "type": "string",
            "description": "Idle time"
          },
          "pending_bytes": {
            "type": "integer",
            "description": "Number of pending bytes"
          },
          "in_msgs": {
            "type": "integer",
            "description": "Number of inbound messages"
          },
          "out_msgs": {
            "type": "integer",
            "description": "Number of outbound messages"
          },
          "in_bytes": {
            "type": "integer",
            "description": "Number of inbound bytes"
          },
          "out_bytes": {
            "type": "integer",
            "description": "Number of outbound bytes"
          },
          "subscriptions": {
            "type": "integer",
            "description": "Number of subscriptions"
          },
          "name": {
            "type": "string",
            "description": "Client name"
          },
          "lang": {
            "type": "string",
            "description": "Client language"
          },
          "version": {
            "type": "string",
            "description": "Client version"
          },
          "tls_version": {
            "type": "string",
            "description": "TLS version"
          },
          "tls_cipher_suite": {
            "type": "string",
            "description": "TLS cipher suite"
          },
          "tls_peer_certs": {
            "type": "array",
            "description": "TLS peer certificates",
            "items": {
              "type": "object",
              "properties": {
                "subject": {
                  "type": "string"
                },
                "spki_sha256": {
                  "type": "string"
                },
                "cert_sha256": {
                  "type": "string"
                }
              }
            }
          },
          "tls_first": {
            "type": "boolean",
            "description": "Whether TLS was established first"
          },
          "authorized_user": {
            "type": "string",
            "description": "Authorized username"
          },
          "account": {
            "type": "string",
            "description": "Account name"
          },
          "subscriptions_list": {
            "type": "array",
            "description": "List of subscription subjects",
            "items": {
              "type": "string"
            }
          },
          "subscriptions_list_detail": {
            "type": "array",
            "description": "Detailed subscription information",
            "items": {
              "type": "object"
            }
          },
          "jwt": {
            "type": "string",
            "description": "JWT token"
          },
          "issuer_key": {
            "type": "string",
            "description": "JWT issuer key"
          },
          "name_tag": {
            "type": "string",
            "description": "Connection name tag"
          },
          "tags": {
            "type": "array",
            "description": "Connection tags",
            "items": {
              "type": "string"
            }
          },
          "mqtt_client": {
            "type": "string",
            "description": "MQTT client ID"
          },
          "proxy": {
            "type": "object",
            "properties": {
              "key": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  },
