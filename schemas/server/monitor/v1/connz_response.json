        },
        "tags": {
          "type": "array",
          "description": "Connection tags",
          "items": {
            "type": "string"
          }
        },
        "mqtt_client": {
          "type": "string",
          "description": "MQTT client ID"
        },
        "proxy": {
          "type": "object",
