{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://nats.io/schemas/server/monitor/v1/profilez_request.json",
  "description": "A request to the NATS $SYS monitoring endpoint for profiling information",
  "title": "io.nats.server.monitor.v1.profilez_request",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "enum": [
        "allocs",
        "block",
        "goroutine",
        "heap",
        "mutex",
        "threadcreate",
        "cpu"
      ],
      "description": "The type of profile to generate"
    },
    "debug": {
      "type": "integer",
      "minimum": 0,
      "description": "Debug level for the profile"
    },
    "duration": {
      "type": "integer",
      "minimum": 0,
      "description": "Duration in seconds for CPU profiling"
    }
  }
}
